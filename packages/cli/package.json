{
  "name": "@kagaribi/cli",
  "version": "0.2.0",
  "description": "CLI tool for kagaribi microservices framework",
  "license": "MIT",
  "type": "module",
  "bin": {
    "kagaribi": "./dist/bin.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./bin": "./dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goofmint/kagaribi.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/goofmint/kagaribi",
  "keywords": [
    "hono",
    "microservices",
    "cli",
    "deploy",
    "cloudflare-workers",
    "aws-lambda"
  ],
  "engines": {
    "node": ">=22.6.0"
  },
  "scripts": {
    "build": "tsup src/bin.ts src/index.ts --format esm --dts --external esbuild",
    "dev": "tsup src/bin.ts src/index.ts --format esm --dts --watch --external esbuild",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@kagaribi/core": "workspace:*",
    "hono": "^4.0.0",
    "@hono/node-server": "^1.0.0",
    "esbuild": "^0.24.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
