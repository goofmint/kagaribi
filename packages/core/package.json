{
  "name": "@kagaribi/core",
  "version": "0.1.0",
  "description": "Core library for kagaribi microservices framework",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goofmint/kagaribi.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/goofmint/kagaribi",
  "keywords": [
    "hono",
    "microservices",
    "monorepo",
    "serverless",
    "cloudflare-workers",
    "aws-lambda"
  ],
  "engines": {
    "node": ">=22.6.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "esbuild": "^0.27.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  }
}
